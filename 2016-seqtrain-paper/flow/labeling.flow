<?xml version="1.0" encoding="ISO-8859-1"?>
<network>
  <out name="energies"/>
  <out name="features"/>
  <out name="labels"/>

  <param name="id"/>
  <param name="input-file"/>
  <param name="track"/>
  <param name="start-time"/>
  <param name="end-time"/>
  <param name="speaker"/>



  <node name="feature-extraction-setup" 
	filter="$(file)"
	id="$(id)" 
        input-file="$(input-file)" 
	track="$(track)"
	start-time="$(start-time)" 
	end-time="$(end-time)" 
	speaker="$(speaker)"/>


  <node name="labels" 
	filter="generic-coprus-key-map" 
	key="$(id)"
	start-time="$(start-time)" 
	end-time="$(end-time)"/>


  <link from="feature-extraction-setup:energies" to="network:energies"/>
  <link from="feature-extraction-setup:features" to="network:features"/>
  <link from="labels" to="network:labels"/>

</network>
